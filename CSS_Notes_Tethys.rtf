\f0\b\fs28 \cf2 1. The API Route: 
\f3\fs30\fsmilli15210 src/app/api/tethys/sign_slate/route.js
\f0\fs28 \
\pard\pardeftab720\sa240\partightenfactor0

\f1\b0\fs24 \cf2 This route securely accepts the signature from the client, adds your 
\f0\b Application Password
\f1\b0  (so the user doesn't need to be an admin), and posts it to the 
\f2\fs26 guest_signature
\f1\fs24  Custom Post Type in WordPress.\
\pard\pardeftab720\partightenfactor0
\cf2 JavaScript\
\
\pard\pardeftab720\partightenfactor0

\f2\fs26 \cf2 import \{ NextResponse \} from 'next/server';\
\
export async function POST(request) \{\
  const wpBase = process.env.NEXT_PUBLIC_WP_URL?.replace(/\\/$/, '');\
  const wpUser = process.env.WP_USER;\
  const wpAppPass = process.env.WP_APP_PASS;\
\
  if (!wpBase || !wpUser || !wpAppPass) \{\
    return NextResponse.json(\
      \{ error: 'Server configuration missing (WP Credentials)' \},\
      \{ status: 500 \}\
    );\
  \}\
\
  try \{\
    const body = await request.json();\
    const \{ title, content \} = body;\
\
    if (!title || !content) \{\
      return NextResponse.json(\{ error: 'Signature requires name and message.' \}, \{ status: 400 \});\
    \}\
\
    // Secure Auth Header\
    const auth = Buffer.from(`$\{wpUser\}:$\{wpAppPass\}`).toString('base64');\
\
    // Post to the 'guest_signature' CPT endpoint\
    // Ensure you have created this Post Type in CPT UI with REST API enabled!\
    const targetUrl = `$\{wpBase\}/wp-json/wp/v2/guest_signature`;\
\
    const wpRes = await fetch(targetUrl, \{\
      method: 'POST',\
      headers: \{\
        'Authorization': `Basic $\{auth\}`,\
        'Content-Type': 'application/json',\
      \},\
      body: JSON.stringify(\{\
        title: title,        // The User's Name\
        content: content,    // Their "I was here" message\
        status: 'publish',   // 'publish' for instant appearing, or 'pending' for moderation\
      \}),\
    \});\
\
    if (!wpRes.ok) \{\
      const errorText = await wpRes.text();\
      console.error('WP Signature Error:', errorText);\
      return NextResponse.json(\
        \{ error: 'The slate rejected your mark. (CPT configuration error?)' \}, \
        \{ status: wpRes.status \}\
      );\
    \}\
\
    const data = await wpRes.json();\
    return NextResponse.json(\{ success: true, id: data.id \}, \{ status: 201 \});\
\
  \} catch (error) \{\
    console.error('Slate Proxy Error:', error);\
    return NextResponse.json(\{ error: 'Failed to etch signature.' \}, \{ status: 500 \});\
  \}\
\}\
