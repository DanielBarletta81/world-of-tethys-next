export function useExpeditionSync(userData, currentProgress) \{\
  useEffect(() => \{\
    const syncInterval = setInterval(async () => \{\
      if (!userData) return;\
\
      await fetch('https://cms.dcbarletta.com/wp-json/tethys/v1/save-progress', \{\
        method: 'POST',\
        headers: \{ 'Content-Type': 'application/json' \},\
