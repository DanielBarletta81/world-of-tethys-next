    \cf15 \cb7 localStorage\cf6 \cb7 .setItem(\cf10 \cb7 'tethys_inventory'\cf6 \cb7 , \cf15 \cb7 JSON\cf6 \cb7 .stringify([...inventory, item]));\
  \};\
\
