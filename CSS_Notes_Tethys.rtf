\f2\fs26 \cf2 /**\
 * Register a custom REST API route to save discovery progress\
 */\
add_action('rest_api_init', function () \{\
    register_rest_route('tethys/v1', '/save-progress', array(\
        'methods' => 'POST',\
        'callback' => 'handle_expedition_progress',\
        'permission_callback' => function () \{\
            // In production, verify JWT or Application Password here\
            return true; \
        \}\
    ));\
\});\
\
function handle_expedition_progress($request) \{\
    $user_id = $request->get_param('user_id'); // From Auth0/WP user mapping\
    $discovery_count = $request->get_param('discovery_count');\
    $unlocked_items = $request->get_param('inventory'); // Array of items\
\
    // Save to WordPress User Meta\
    update_user_meta($user_id, 'tethys_discovery_points', $discovery_count);\
    update_user_meta($user_id, 'tethys_inventory', $unlocked_items);\
\
    return new WP_REST_Response(array('status' => 'Archived in Compendium'), 200);\
\}\
