\cf9 \cb7 export\cf6 \cb7  \cf9 \cb7 function\cf6 \cb7  \cf11 \cb7 useExpedition\cf6 \cb7 () \{\
  \cf9 \cb7 const\cf6 \cb7  [sessionTime, setSessionTime] = useState(\cf13 \cb7 0\cf6 \cb7 );\
  \cf9 \cb7 const\cf6 \cb7  [inventory, setInventory] = useState([]);\
\
  useEffect(() => \{\
    \cf9 \cb7 const\cf6 \cb7  timer = \cf15 \cb7 setInterval\cf6 \cb7 (() => \{\
      setSessionTime(prev => \{\
